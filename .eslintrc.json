{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "import/no-extraneous-dependencies" : "off",
        "indent" : ["error", 2],
        "quotes" : ["error", "single"],
        "semi" : ["error", "always"],
        "max-len" : [
            "error", {
            "code": 120
        }]
    }
}